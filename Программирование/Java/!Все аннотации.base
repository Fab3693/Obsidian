formulas:
  Untitled: contains()
views:
  - type: table
    name: Таблица
    filters:
      and:
        - file.name.startsWith("@")
    order:
      - file.name
      - formula.Untitled
    sort:
      - property: formula.Untitled
        direction: ASC
    imageFit: contain
    imageAspectRatio: 1.45
    cardSize: 400
  - type: cards
    name: представление
